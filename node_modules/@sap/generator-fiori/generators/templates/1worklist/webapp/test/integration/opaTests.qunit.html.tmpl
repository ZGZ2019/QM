<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<title>Integration tests for {{1worklist.parameters.ApplicationTitle.value}}</title>

	<script id="sap-ui-bootstrap"
{{#if isModule}}
			src="https://sapui5.hana.ondemand.com/resources/sap-ui-core.js"
{{else}}
			src="{{1worklist.environment.resourcePath}}../../resources/sap-ui-core.js"
{{/if}}
			data-sap-ui-theme="sap_fiori_3"
			data-sap-ui-resourceroots='{
				"{{1worklist.parameters.AppId.value}}": "../../",
				"{{1worklist.parameters.AppId.value}}.test": "../"
			}'
			data-sap-ui-animation="false"
			data-sap-ui-compatVersion="edge"
			data-sap-ui-async="true"
            data-sap-ui-preload="async">
	</script>

	<link rel="stylesheet" type="text/css" href="{{1worklist.environment.resourcePath}}../../resources/sap/ui/thirdparty/qunit-2.css">

	<script src="{{1worklist.environment.resourcePath}}../../resources/sap/ui/thirdparty/qunit-2.js"></script>
	<script src="{{1worklist.environment.resourcePath}}../../resources/sap/ui/qunit/qunit-junit.js"></script>

	<script src="opaTests.qunit.js"></script>

</head>
<body>
	<div id="qunit"></div>
	<div id="qunit-fixture"></div>
</body>
</html>
